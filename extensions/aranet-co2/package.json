{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "aranet-co2",
  "title": "Aranet CO2",
  "description": "Display Aranet4 Data",
  "icon": "command-icon.png",
  "author": "williamwelsh",
  "license": "MIT",
  "commands": [
    {
      "name": "aranet",
      "title": "Show Aranet Data",
      "description": "Display CO₂ and other Aranet Data",
      "mode": "view"
    },
    {
      "name": "aranet-co2-menu-bar",
      "title": "Aranet Menu Bar",
      "description": "Display CO₂ in your menu bar",
      "mode": "menu-bar",
      "interval": "5m"
    }
  ],
  "preferences": [
    {
      "name": "temperature",
      "type": "dropdown",
      "required": true,
      "title": "Temperature Type",
      "description": "Fahrenheit or Celsius",
      "data": [
        {
          "title": "Fahrenheit",
          "value": "fahrenheit"
        },
        {
          "title": "Celsius",
          "value": "celsius"
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.64.4",
    "shell-env": "^4.0.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
